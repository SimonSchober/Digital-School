 var speed= 4;
var x = 300;
var y = 20;


function setup() {
  createCanvas(600, 400);
}

function draw() {


  
  stroke(200, 100, 0)
  strokeWeight(2);

var random = rndInt (1, 4);
if(random === 1){
     
    y = y - speed;
}   else if  (random === 2){
    y = y + speed;
}   else if  (random === 3){
    x = x + speed;
}   else if  (random === 4){
    x = x - speed;
}


   
   rect(x,y,40,40);

}

function rndInt(min, max) {
            return Math.floor(Math.random() * (max - min + 1)) + min;
        } 


